\begin{lstlisting}[caption=Ex\_02\_02.js]
     1: function setup() {
     2:   createCanvas(480, 120);
     3: }
     4: function draw() {
     5:   if (mouseIsPressed) {
     6:     fill(0);
     7:   } else {
     8:     fill(255);
     9:   }
    10:   ellipse(mouseX, mouseY, 80, 80);
    11: }
\end{lstlisting}
