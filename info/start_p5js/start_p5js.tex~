\begin{lstlisting}[caption=Ex\_11\_10.js]
     1: var bugs = [];
     2: function setup() {
     3:   createCanvas(240, 120);
     4:   background(204);
     5:   for (var i = 0; i < 33; i++) {
     6:     var x = random(width);
