     1: function setup() {
     2:   createCanvas(480, 120);
     3:   noLoop();
     4:   background(204);
     5:   fill(255);
     6: } 
     7: function draw() {
     8:   ellipse(75, 60, 90, 90);
     9:   strokeWeight(8);
    10:   ellipse(175, 60, 90, 90);
    11:   ellipse(279, 60, 90, 90);
    12:   strokeWeight(20);
    13:   ellipse(389, 60, 90, 90);
    14: }
