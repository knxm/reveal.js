\begin{lstlisting}[caption=Ex\_03\_12.js]
     1: function setup() {
     2:   createCanvas(480, 120);
     3:   strokeWeight(24);
     4: }
     5: function draw() {
     6:   background(204);
     7:   line(60, 25, 130, 95);
     8:   strokeCap(SQUARE);
     9:   line(160, 25, 230, 95);
    10:   strokeCap(PROJECT);
    11:   line(260, 25, 330, 95);
    12:   strokeCap(ROUND);
    13:   line(360, 25, 430, 95);
    14: }
\end{lstlisting}
