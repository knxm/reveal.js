\begin{lstlisting}[caption=Ex\_05\_08.js]
     1: var x = 0;
     2: var easing = 0.01;
     3: function setup() {
     4:   createCanvas(220, 120);
     5: }
     6: function draw() {
     7:   var targetX = mouseX;
     8:   x += (targetX - x) * easing;
     9:   ellipse(x, 40, 12, 12);
    10:   print(targetX + " : " + x);
    11: }
\end{lstlisting}
