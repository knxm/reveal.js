     1: function setup() {
     2:   createCanvas(480, 120);
     3:   strokeWeight(12);
     4: }
     5: function draw() {
     6:   background(204);
     7:   rect(60, 25, 70, 70);
     8:   strokeJoin(ROUND);
     9:   rect(160, 25, 70, 70);
    10:   strokeJoin(BEVEL);
    11:   rect(260, 25, 70, 70);
    12:   strokeJoin(MITER);
    13:   rect(360, 25, 70, 70);
    14: }
\end{lstlisting}
\includegraphics[height=3cm]{image/Ex_03_13.pdf}
\begin{lstlisting}[caption=Ex\_03\_14.js]
     1: function setup() {
     2:   createCanvas(480, 120);
     3:   noLoop();
     4: }
     5: function draw() {
     6:   background(204);
     7:   fill(255);
     8:   rect(120, 60, 80, 80);
     9:   ellipse(120, 60, 80, 80);
    10:   ellipseMode(CORNER);
    11:   rect(280, 20, 80, 80);
    12:   ellipse(280, 20, 80, 80);
    13: }
\end{lstlisting}
