\begin{lstlisting}[caption=Ex\_05\_09.js]
     1: var x = 0;
     2: var y = 0;
     3: var px = 0;
     4: var py = 0;
     5: var easing = 0.05;
     6: function setup() {
     7:   createCanvas(480, 120);
     8:   stroke(0, 102);
     9: }
    10: function draw() {
    11:   var targetX = mouseX;
    12:   x += (targetX - x) * easing;
    13:   var targetY = mouseY;
    14:   y += (targetY - y) * easing;
    15:   var weight = dist(x, y, px, py);
    16:   strokeWeight(weight);
    17:   line(x, y, px, py);
    18:   py = y;
    19:   px = x;
    20: }
\end{lstlisting}
\vspace{1in}
